# Function to check if pair 
# with given sum exists
def pair_sum(dict, N, arr, sum):
    
    # Your code here
    # Hint: You can use 'in' to find if any key is in dict
    for i in range(N):
        for j in range(i+1,N):
            if (arr[i]+arr[j] == sum):
               
                return True 
    
    return False
