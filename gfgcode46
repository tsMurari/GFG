

//User function Template for C++

// Function to swap A and B
// using pointers
void swap(int *A, int *B){
    
    // Your code here
    int temp=*A;
    *A=*B;
    *B= temp;
    
}

// Function to swap A and B
// using reference
void swap(int &A, int &B){
  
    // Your code here
    int temp=B;
    B=A;
    A=temp;
  
}
