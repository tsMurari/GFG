class Solution {
  public:
    int cppIntType() {
        // code here
        int x; cin >> x; return x;
    }
    
    char cppCharType() {
        // code here
        char x; cin >> x; return x;
    }
    
    float cppFloatType() {
        // code here
        float x; cin >> x; return x;
    }
};
